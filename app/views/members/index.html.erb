<br><div class="card">
  <p style="color: green"><%= notice %></p>

<% content_for :title, "Members" %>

<h1>Members</h1>


<div id="members">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Action</th>
      </tr>
    </thead>
  </table>
  <% @members.each do |member| %>
    <%= render member %>
    
  <% end %>
</div><br>
<%= link_to "New member", new_member_path,class: "btn btn-primary" %>
</div>
